    uint256 public mintPrice;
    string public baseURI;

    modifier isMintable(uint256 _tokenId) {
        require(_tokenId <= tokenLimit, "Token invalid");
        require(_tokenId > 0, "Token invalid");
        require(msg.value >= mintPrice, "Payment error");
        _;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function mint(uint256 _tokenId)
        public
        virtual
        payable
        isMintable(_tokenId)
    {
        _safeMint(_msgSender(), _tokenId);
    }

    function setMintPrice(uint256 _mintPrice)
        external
        onlyOwner
    {
        mintPrice = _mintPrice;
    }

    function setTokenLimit(uint256 _tokenLimit)
        external
        onlyOwner
    {
        tokenLimit = _tokenLimit;
    }

    function setBaseURI(string calldata _baseURI)
        external
        onlyOwner
    {
        baseURI = _baseURI;
    }

    function withdraw()
        external
        onlyOwner
    {
        uint256 balance = address(this).balance;
        payable(_msgSender()).transfer(balance);
    }

    constructor(string memory _name, string memory _symbol, uint256 _tokenLimit, uint256 _mintPrice, string memory _baseURI)
        ERC721(_name, _symbol)
    {
        tokenLimit = _tokenLimit;
        mintPrice = _mintPrice;
        baseURI = _baseURI;
    }

}

abstract contract OnChainDataNFT is MintableNFT {

    mapping(uint256 => bytes) private names;
    mapping(uint256 => bytes) private descriptions;
    mapping(uint256 => bytes) private contents;

    event DataChanged(address indexed from, uint256 indexed tokenId);

    modifier tokenOwnerOnly(uint256 _tokenId) {
        address owner = ERC721.ownerOf(_tokenId);
        require(owner == _msgSender(), "Caller is not the owner");
        _;
    }

    function setTokenName(uint256 _tokenId, bytes calldata _name)
        external
        tokenOwnerOnly(_tokenId)
    {
        names[_tokenId] = _name;
        emit DataChanged(_msgSender(), _tokenId);
    }

    function setTokenDescription(uint256 _tokenId, bytes calldata _description)
        external
        tokenOwnerOnly(_tokenId)
    {
        descriptions[_tokenId] = _description;
        emit DataChanged(_msgSender(), _tokenId);
    }

    function setTokenContent(uint256 _tokenId, bytes calldata _content)
        external
        tokenOwnerOnly(_tokenId)
    {
        contents[_tokenId] = _content;
        emit DataChanged(_msgSender(), _tokenId);
    }

    function setTokeData(uint256 _tokenId, bytes calldata _name, bytes calldata _description, bytes calldata _content)
        external
        tokenOwnerOnly(_tokenId)
    {
        names[_tokenId] = _name;
        descriptions[_tokenId] = _description;
        contents[_tokenId] = _content;
        emit DataChanged(_msgSender(), _tokenId);
    }

    function getTokenData(uint256 _tokenId) view virtual public returns(bytes memory, bytes memory, bytes memory) {
        return (names[_tokenId], descriptions[_tokenId], contents[_tokenId]);
    }

}

contract Matunity is OnChainDataNFT {

    mapping(address => uint256) private points;
    mapping(address => uint256) private locations;
    mapping(address => uint256) private lastGames;
    mapping(address => uint256) private lastRools;
    mapping(uint256 => uint256) private earningsNFT;
    mapping(uint256 => uint256) private claimsNFT;

    uint256 public roolPrice;
    uint256 public pointPrice;
    uint256 public pointsPerGame;
    uint256 public cooldown;
    uint256 public prizePool;
    uint256 public devsPool;
    uint256 public mintsPool;
    uint256 public pointsForPrize;
    uint256 public prizePercentage;
    uint256 public commissionsNFT;
    uint256 public commissionsDevs;
    uint256 public commissionsPool;
    uint256 public commissionsMintPool;

    event Rool(address indexed player, uint256 rool);
    event Won(address indexed player, uint256 points);
    event Earned(uint256 indexed tokenId, uint256 amount);
    event ClaimedEarnings(uint256 indexed tokenId, uint256 amount);
    event ClaimedPrize(address indexed player, uint256 amount);

    constructor(string memory _name, string memory _symbol, uint256 _tokenLimit, uint256 _mintPrice, string memory _baseURI, uint _roolPrice, uint _pointPrice, uint _pointsPerGame, uint256 _cooldown, uint _pointsForPrize, uint _prizePercentage)
        MintableNFT(_name, _symbol, _tokenLimit, _mintPrice, _baseURI)
    {
        prizePool = 0;
        devsPool = 0;
        mintsPool = 0;
        commissionsNFT = 20;
        commissionsDevs = 20;
        commissionsPool = 60;
        commissionsMintPool = 80;
        roolPrice = _roolPrice;
        pointPrice = _pointPrice;
        pointsPerGame = _pointsPerGame;
        cooldown = _cooldown;
        pointsForPrize = _pointsForPrize;
        prizePercentage = _prizePercentage;
    }

    function fund()
        external
        payable
    {
        require(msg.value > 0, "Funding error");
        prizePool = prizePool + msg.value;
    }

    function setCommissionsNFT(uint256 _commissionsNFT)
        external
        onlyOwner
    {
        commissionsNFT = _commissionsNFT;
    }

    function setCommissionsDevs(uint256 _commissionsDevs)
        external
        onlyOwner
    {
        commissionsDevs = _commissionsDevs;
    }

    function setCommissionsPool(uint256 _commissionsPool)
        external
        onlyOwner
    {
        commissionsPool = _commissionsPool;
    }

    function setCommissionsMintPool(uint256 _commissionsMintPool)
        external
        onlyOwner
    {
        commissionsMintPool = _commissionsMintPool;
    }

    function setRoolPrice(uint256 _roolPrice)
        external
        onlyOwner
    {
        roolPrice = _roolPrice;
    }

    function setPointPrice(uint256 _pointPrice)
        external
        onlyOwner
    {
        pointPrice = _pointPrice;
    }

    function setPointsPerGame(uint256 _pointsPerGame)
        external
        onlyOwner
    {
        pointsPerGame = _pointsPerGame;
    }

    function setCooldown(uint256 _cooldown)
        external
        onlyOwner
    {
        cooldown = _cooldown;
    }

    function setPointsForPrize(uint256 _pointsForPrize)
        external
        onlyOwner
    {
        pointsForPrize = _pointsForPrize;
    }

    function setPrizePercentage(uint256 _prizePercentage)
        external
        onlyOwner
    {
        prizePercentage = _prizePercentage;
    }

    modifier isEligableToRool(address _player) {
        require(msg.value >= roolPrice, "Payment error");
        if (lastGames[_player] > 0) {
            require(lastGames[_player] + cooldown <= block.timestamp, "Cooldown is still valid");
        }
        _;
    }

    function rool(uint256 _randomAdd)
        external
        payable
        isEligableToRool(_msgSender())
        returns (uint256, uint256, bool)
    {
        uint256 _rool = (_randomAdd + block.timestamp) % 6 + 1;
        bool _won = false;

        emit Rool(_msgSender(), _rool);

        if (locations[_msgSender()] > 0) {
            locations[_msgSender()] = locations[_msgSender()] + _rool;
            if (locations[_msgSender()] > tokenLimit) {
                locations[_msgSender()] = 0;
                points[_msgSender()] = points[_msgSender()] + pointsPerGame;
                lastGames[_msgSender()] = block.timestamp;
                _won = true;

                emit Won(_msgSender(), pointsPerGame);
            }
        } else {
            locations[_msgSender()] = _rool;
        }
        lastRools[_msgSender()] = _rool;

        if (locations[_msgSender()] > 0) {
            uint256 _amount = msg.value * commissionsNFT / 10000;
            emit Earned(locations[_msgSender()], _amount);
            earningsNFT[locations[_msgSender()]] = earningsNFT[locations[_msgSender()]] + _amount;
            devsPool = devsPool + msg.value * commissionsDevs / 10000;
            prizePool = prizePool + msg.value * commissionsPool / 10000;
        } else {
            //no nft so funds go to prize pool
            prizePool = prizePool + msg.value * commissionsNFT / 10000;
            devsPool = devsPool + msg.value * commissionsDevs / 10000;
            prizePool = prizePool + msg.value * commissionsPool / 10000;
        }

        return (_rool, locations[_msgSender()], _won);
    }

    function getPlayerData(address _player) view public returns(uint256, uint256, uint256, uint256, bool) {
        bool _cooldown = false;
        if (lastGames[_player] > 0 && lastGames[_player] + cooldown > block.timestamp) {
            _cooldown = true;
        }
        return (lastRools[_player], locations[_player], points[_player], lastGames[_player], _cooldown);
    }

    function mint(uint256 _tokenId)
        public
        virtual
        payable
        override
    {
        super.mint(_tokenId);
        mintsPool = mintsPool + msg.value;
    }

    function claimDevs()
        external
        onlyOwner
    {
        payable(_msgSender()).transfer(devsPool);

        devsPool = 0;
    }

    function claimMints(uint256 _amount)
        external
        onlyOwner
    {
        if (_amount > mintsPool) {
            _amount = mintsPool;
        }
        mintsPool = mintsPool - _amount;
        prizePool = prizePool + _amount * commissionsMintPool / 10000;

        payable(_msgSender()).transfer(_amount - _amount * commissionsMintPool / 10000);
    }

    function claimEarnings(uint256 _tokenId)
        external
        tokenOwnerOnly(_tokenId)
    {
        require(earningsNFT[_tokenId] > claimsNFT[_tokenId], "Nothing to claim");
        uint256 _amount = earningsNFT[_tokenId] - claimsNFT[_tokenId];
        claimsNFT[_tokenId] = earningsNFT[_tokenId];

        emit ClaimedEarnings(_tokenId, _amount);
        payable(_msgSender()).transfer(_amount);
    }

    function getAllTokenData(uint256 _tokenId) view public returns(bytes memory, bytes memory, bytes memory, uint256, uint256) {
        (bytes memory _name, bytes memory _description, bytes memory _content) = super.getTokenData(_tokenId);
        return (_name, _description, _content, earningsNFT[_tokenId], claimsNFT[_tokenId]);
    }

    modifier isEligableForPrize(address _player) {
        require(points[_player] >= pointsForPrize, "Insufieicient points");
        _;
    }

    function claimPrize()
        external
        isEligableForPrize(_msgSender())
    {
        uint256 _amount = prizePool * prizePercentage / 10000;
        prizePool = prizePool - _amount;

        points[_msgSender()] = points[_msgSender()] - pointsForPrize;
        emit ClaimedPrize(_msgSender(), _amount);
        payable(_msgSender()).transfer(_amount);
    }

    function getPrizeData() view public returns(uint256, uint256) {
        return(prizePool, prizePool * prizePercentage / 10000);
    }


    modifier isMintableForPoints(uint256 _tokenId) {
        require(_tokenId <= tokenLimit, "Token invalid");
        require(_tokenId > 0, "Token invalid");
        require(points[_msgSender()] >= pointPrice, "Payment error");
        _;
    }

    function mintForPoints(uint256 _tokenId)
        external
        virtual
        isMintableForPoints(_tokenId)
    {
        points[_msgSender()] = points[_msgSender()] - pointPrice;
        _safeMint(_msgSender(), _tokenId);
    }

}


